{
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    "**/.hg/store/**": true,
    "**/.vite/**": true
  },
  "search.exclude": {
    "**/node_modules/**": true,
    "**/bower_components/**": true,
    "**/*.code-search": true,
    "**/.vite/**": true
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.check.npmIsTSVersion": true
}